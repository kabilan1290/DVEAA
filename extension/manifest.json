{
  "manifest_version": 3,
  "name": "Shadow DOM Data Extractor - Hospital Demo",
  "description": "CTF Demo: Extracts sensitive data from Shadow DOM in hospital systems",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "scripting",
    "dom",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Shadow DOM Extractor",
    "default_icon": {
      "16": "data:image/png;base64,",
      "48": "data:image/png;base64,",
      "128": "data:image/png;base64,"
    }
  },
  "host_permissions": [
    "*://localhost/*",
    "*://0.0.0.0/*"
  ]
}
